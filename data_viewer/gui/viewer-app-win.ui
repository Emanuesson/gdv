<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.8 -->
<!--  <requires lib="gtk+" version="3.14"/> -->
  <template class="GdvViewerAppWindow" parent="GtkApplicationWindow">
    <property name="title" translatable="yes">Gdv Application</property>
    <property name="window_position">center</property>
    <property name="default_width">800</property>
    <property name="default_height">600</property>
    <property name="height_request">600</property>
    <child>
      <object class="GtkBox" id="content_box">
        <property name="visible">True</property>
        <property name="orientation">vertical</property>
        <child>
          <object class="GdvTwodLayer" id="MainLayer">
            <property name="visible">True</property>
          </object>
        </child>
        <child>
<!--          <object class="Gdv2dView" id="view">
            <property name="visible">True</property>
          <child>
            <object class="Gdv2dLayer" id="layer">-->
<!--            <child>
              <object class="GdvLayerContent" id="content">
              </object>
            </child>-->
<!--            </object>
          </child>
          </object>-->
        </child>
      </object>
    </child>
  </template>
</interface>
