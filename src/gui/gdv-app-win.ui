<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.8 -->
  <!--  <requires lib="gtk+" version="3.14"/> -->
  <template class="GdvViewerAppWindow" parent="GtkApplicationWindow">
    <property name="title" translatable="yes">Gdv Application</property>
    <property name="window_position">center</property>
    <property name="default_width">800</property>
    <property name="decorated">True</property>
    <property name="has-resize-grip">True</property>
    <property name="default_height">600</property>
    <child>

      <object class="GtkBox">
        <property name="visible">True</property>
        <property name="orientation">vertical</property>


        <child>

          <object class="GdvSpecialDrumDisplay" id="main_layer">
            <property name="visible">True</property>
          </object>

          <packing>
            <property name="fill">True</property>
            <property name="expand">True</property>
          </packing>
        </child>
      </object>
    </child>
  </template>
</interface>

