<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.8 -->
  <!--  <requires lib="gtk+" version="3.14"/> -->

  <template class="GdvInspectorAppWindow" parent="GtkApplicationWindow">
    <property name="title" translatable="yes">Gdv Inspector</property>
    <property name="window_position">center</property>
    <property name="hide-titlebar-when-maximized">FALSE</property>
    <property name="default_width">800</property>
    <property name="default_height">500</property>
    <child>

      <object class="GtkNotebook" id="notebook">
        <property name="visible">True</property>
        <property name="tab-pos">GTK_POS_LEFT</property>
        <property name="scrollable">True</property>

        <child>
          <object class="GdvSpecialDrumDisplay" id="onedlayer">
            <property name="visible">True</property>
            <property name="margin">10</property>
          </object>
          <packing>
            <property name="tab-label">1D-Layer</property>
          </packing>
        </child>

        <child>
          <object class="GdvOnedLayer" id="loglayer">
            <property name="visible">True</property>
          </object>
          <packing>
            <property name="tab-label">1D-Layer (log)</property>
          </packing>
        </child>

        <child>
          <object class="GdvTwodLayer" id="twodlayer">
            <property name="visible">True</property>
          </object>
          <packing>
            <property name="tab-label">2D-Layer</property>
          </packing>
        </child>

        <child>
          <object class="GdvInspectorTest2" id="test2">
            <property name="visible">True</property>
          </object>
          <packing>
            <property name="tab-label">PolarAxis</property>
          </packing>
        </child>

        <child>
          <object class="GdvInspectorTest3" id="test3">
            <property name="visible">True</property>
          </object>
          <packing>
            <property name="tab-label">Test3</property>
          </packing>
        </child>

        <child>
          <object class="GdvInspectorTest4" id="test4">
            <property name="visible">True</property>
          </object>
          <packing>
            <property name="tab-label">Test4</property>
          </packing>
        </child>

        <child>
          <object class="GdvInspectorTest5a" id="test5a">
            <property name="visible">True</property>
          </object>
          <packing>
            <property name="tab-label">Test5a</property>
          </packing>
        </child>

        <child>
          <object class="GdvInspectorTest6" id="test6">
            <property name="visible">True</property>
          </object>
          <packing>
            <property name="tab-label">Test6</property>
          </packing>
        </child>

        <child>
          <object class="GdvInspectorTest7" id="test7">
            <property name="visible">True</property>
          </object>
          <packing>
            <property name="tab-label">Test7</property>
          </packing>
        </child>

        <child>
          <object class="GdvInspectorTest8" id="test8">
            <property name="visible">True</property>
          </object>
          <packing>
            <property name="tab-label">Test8</property>
          </packing>
        </child>

        <child>
          <object class="GdvInspectorTest9" id="test9">
            <property name="visible">True</property>
          </object>
          <packing>
            <property name="tab-label">Test9</property>
          </packing>
        </child>

        <child>
          <object class="GdvInspectorTest10" id="test10">
            <property name="visible">True</property>
          </object>
          <packing>
            <property name="tab-label">Test10</property>
          </packing>
        </child>

      </object>
    </child>

  </template>
</interface>

