<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.8 -->
  <!--  <requires lib="gtk+" version="3.14"/> -->

  <template class="GdvInspectorAppWindow" parent="GtkApplicationWindow">
    <property name="title" translatable="yes">Gdv Inspector</property>
    <property name="window_position">center</property>
    <property name="hide-titlebar-when-maximized">FALSE</property>
    <property name="default_width">500</property>
    <property name="default_height">300</property>
    <child>

      <object class="GtkNotebook" id="notebook">
        <property name="visible">True</property>
        <property name="tab-pos">GTK_POS_LEFT</property>
        <property name="scrollable">True</property>

        <child>
          <object class="GdvSpecialDrumDisplay" id="onedlayer">
            <property name="visible">True</property>
            <property name="margin">10</property>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel" id="1dLayer-tab">
            <property name="label">1D-Layer</property>
            <property name="visible">True</property>
          </object>
        </child>

        <child>
          <object class="GdvOnedLayer" id="loglayer">
            <property name="visible">True</property>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel" id="1dLayer-log-tab">
            <property name="label">1D-Layer (log)</property>
            <property name="visible">True</property>
          </object>
        </child>

        <child>
          <object class="GdvTwodLayer" id="twodlayer">
            <property name="visible">True</property>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel" id="2dLayer-tab">
            <property name="label">2D-Layer</property>
            <property name="visible">True</property>
          </object>
        </child>

        <child>
          <object class="GdvOnedLayer" id="polarlayer">
            <property name="visible">True</property>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel" id="polaraxis">
            <property name="label">Polar Axis</property>
            <property name="visible">True</property>
          </object>
        </child>

        <child>
          <object class="GdvInspectorTest3" id="test3">
            <property name="visible">True</property>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel" id="test3label">
            <property name="label">Test3</property>
            <property name="visible">True</property>
          </object>
        </child>

        <child>
          <object class="GdvInspectorTest4" id="test4">
            <property name="visible">True</property>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel" id="test4label">
            <property name="label">Test4</property>
            <property name="visible">True</property>
          </object>
        </child>

        <child>
          <object class="GdvInspectorTest5a" id="test5a">
            <property name="visible">True</property>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel" id="test5alabel">
            <property name="label">Test5a</property>
            <property name="visible">True</property>
          </object>
        </child>

        <child>
          <object class="GdvInspectorTest6" id="test6">
            <property name="visible">True</property>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel" id="test6label">
            <property name="label">Test6</property>
            <property name="visible">True</property>
          </object>
        </child>

<!--
        <child>
          <object class="GdvSpecialDrumDisplay" id="drumlayer">
            <property name="visible">True</property>
            <property name="margin">10</property>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel" id="DrumLayer-tab">
            <property name="label">Drum-display</property>
            <property name="visible">True</property>
          </object>
        </child>
-->
      </object>
    </child>

  </template>
</interface>

